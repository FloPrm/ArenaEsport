  <!--<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>-->
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag Ckeditor.cdn_url %>

  <!-- Press Start 2P Font -->
  <link href="https://fonts.googleapis.com/css?family=Press+Start+2P&subset=cyrillic,greek,latin-ext" rel="stylesheet">

  <!-- Lato Font -->
  <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet">

  <!-- Bootstrap styles -->
  <link href="assets/stylesheets/bootstrap-3.3.7-dist/css/bootstrap.css" media="screen" rel="stylesheet" type="text/css">

  <!-- Font Awesome -->
  <link href="assets/stylesheets/css/font-awesome.min.css" media="screen" rel="stylesheet" type="text/css">

  <!-- Lightbox styles -->
  <link href="assets/stylesheets/lightbox/css/lightbox.css" rel="stylesheet">

  <!-- Charts styles -->
  <link href="assets/stylesheets/css/morris.min.css" media="screen" rel="stylesheet" type="text/css">

  <!-- Loaders styles -->
  <link href="assets/stylesheets/loaders.css-master/loaders.min.css" media="screen" rel="stylesheet" type="text/css">

  <!-- Custom styles -->
  <link href="assets/stylesheets/css/styles.min.css" media="screen" rel="stylesheet" type="text/css">

  <!-- Responsive styles -->
  <link href="assets/stylesheets/css/responsive.min.css" media="screen" rel="stylesheet" type="text/css">

  <!-- Modernizr -->
  <script type="text/javascript" src="assets/stylesheets/js/modernizr.custom.js"></script>

</head>


    <% unless current_page?(root_path)%>
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
      <body id="page-top" class="index" style="padding-left: 80px;padding-right: 80px;">
        <%= render "layouts/header" %>
        <%= yield %>
    <%else%>
        <%= yield %>
    <%end%>

    <div id="modal-window" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>

    <div class="col-md-12">
      <%= render "layouts/footer" %>
    </div>

  </body>
</html>


